{
  "name": "copy-of-personal-ai-secretary",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "android:sync": "npm run build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:run": "npm run android:sync && npx cap run android",
    "android:build": "npm run build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:build:debug": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "postinstall": "patch-package && node scripts/check-java.cjs && node scripts/setup-android.cjs",
    "deps:check": "npm outdated",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "deps:update": "npm update",
    "deps:update:interactive": "npx npm-check-updates --interactive",
    "deps:update:minor": "npx npm-check-updates -u --target minor && npm install",
    "deps:update:latest": "npx npm-check-updates -u && npm install",
    "deps:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.5",
    "@capacitor/cli": "^7.4.5",
    "@capacitor/core": "^7.4.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "twilio": "^5.12.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@vitejs/plugin-react": "^5.0.0",
    "patch-package": "^8.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "overrides": {
    "tar": "7.5.7"
  }
}
